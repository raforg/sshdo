		dict(config=config3, user='root', label=none, command='echo any ## ',     auth='disallowed'),
		dict(config=config3, user='root', label=none, command='echo any ## a',    auth='disallowed'),
		dict(config=config3, user='root', label=none, command='echo any ## a ',   auth='disallowed'),
